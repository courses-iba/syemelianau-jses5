describe("test/Task6/test_task6.js", function() {

    it("reverseArray(['A', 'B', 'C']) should return ['C', 'B', 'A']", function() {
        var testArray = ["C", "B", "A"];
        expect(reverseArray(["A", "B", "C"])).toEqual(testArray);
    });

    it("reverseArrayInPlace([1, 2, 3, 4, 5]) should return set [5, 4, 3, 2, 1] in place ", function() {
        var arrayValue = [1, 2, 3, 4, 5];
        reverseArrayInPlace(arrayValue);
        expect(arrayValue).toEqual([5, 4, 3, 2, 1]);
    });

});